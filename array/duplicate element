#include <stdio.h>

int main()
{
    int n,count=0;
    printf("element in  array");
    scanf("%d",&n);
    int arr[n];

    for(int i=0;i<n;i++){
        printf("%d element in array \n",i);
        scanf("%d",&arr[i]);
    }
    
    for(int i=0 ;i<n;i++){
        for(int j =i+1 ;j<n;j++){
            if(arr[i]==arr[j]){
               count++;
               break;
                    }
                        
        }
    }
            printf("\n%d",count);
            
}
