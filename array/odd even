
#include <stdio.h>
int main()
{
    int n,k=0,h=0;
    printf("enter size of arraay ");
    scanf("%d",&n);
    int a[n],odd[n],even[n];
    for (int i = 0; i < n; i++) {
        printf("enter %d element \n",i);
        scanf("%d",&a[i]);
        if(a[i]%2==0){
            even[h]=a[i];
            h++;
        }
        else{
            odd[k]=a[i];
        k++;
        }
    }
    printf("even elements are\n");
    for (int i=0;i<h;i++) {
        printf("%d",even[i]);
    }
    printf("\nodd elements are :- ");
    for (int i=0; i<k;i++) {
        printf("%d",odd[i]);
    }
    return 0;
}

