#include <stdio.h>

int main()
{
    int n,flag=0;
    printf("element in  array");
    scanf("%d",&n);
    int arr[n];

    for(int i=0;i<n;i++){
        printf("%d element in array \n",i);
        scanf("%d",&arr[i]);
    }
    
    for(int i=0 ;i<n;i++){
    flag=1;
        for(int j =i+1 ;j<n;j++){
            if(arr[i]==arr[j] && i!=j)
            {
                flag=0;
            }
     }
     
     if(flag=1)
     printf("\n%d",arr[i]);
    }
}
