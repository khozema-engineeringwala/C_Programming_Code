#include <stdio.h>
 int main(){
    int n,m,i,j;
    printf("number row and column");
    scanf("%d%d",&n,&m);
    int arr[n][m];
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            printf("values of matrix arr[%d][%d]",i,j);
            scanf("%d",&arr[i][j]);
            
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            printf("%d",arr[i][j]);
    }
    printf("  \n   ");
    }
    return 0;
    }
