#include <stdio.h>
 int main(){
    int n,m,i,j;
    printf("number row and column");
    scanf("%d%d",&n,&m);
    int arr[n][m];
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            printf("values of matrix arr[%d][%d]",i,j);
            scanf("%d",&arr[i][j]);
            
        }
    }
   for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            printf("%d",arr[i][j]);
    }
    printf(" \n ");
    }
    int t[m][n];
      for(i=0;i<m;i++){
          for(j=0;j<n;j++){
              printf("values of matrix t[%d][%d]",j,i);
          }
           printf(" \n ");
          }
    
    return 0;
    }
