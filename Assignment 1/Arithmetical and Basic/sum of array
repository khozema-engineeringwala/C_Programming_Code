#include <stdio.h>

int main()
{
    int a[];
    int i, n, sum=0;
       printf("elements to be stored in the array :");
       scanf("%d",&n);
   
       printf("%d elements in the array :",n);
       for(i=0;i<n;i++)
        {
	      scanf("%d",&a[i]);
	    }

    for(i=0; i<n; i++)
    {
        sum += a[i];
    }

    printf("Sum of all elements stored in the array is : %d", sum);
}
