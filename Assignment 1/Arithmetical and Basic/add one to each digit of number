#include <stdio.h>

int main() {
    int a,b,c,d,e,num,new;
    printf("enter the num\n");
    scanf("%d",&num);
    a=num%10000/1000;
    b=(num%1000)/100;
    c=(num%100)/10;
    d=num%10;
    e=num/10000;
    new=((e+1)*10000)+((a+1)*1000)+((b+1)*100)+((c+1)*10)+(d+1);
    printf("reverse number  %d",new);
    return 0;
}
