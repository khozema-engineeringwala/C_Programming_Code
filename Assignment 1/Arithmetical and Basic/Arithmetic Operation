//Q3 Write a program to Perform Addition of two No entered through the Keyboard.

#include<stdio.h>

int main(){

  int a=110,b=786,sum;
  
  printf("value of a is %d\n",a);
  printf("value of b is %d\n",b);
  printf("sum of a and b is is %d\n",(a+b));

  return 0;
}

//Q4 Write a Program to calculate Simple Interest for any Principle Amount.

#include<stdio.h>

int main()
{

  int P,R,T;
  
  printf("enter the principle amount\n");
  scanf("%d",&P);
  
  printf("the value rate\n");
  scanf("%d",&R);
  
  printf("the value time\n");
  scanf("%d",&T);
  
  int SI=(P*T*R)/100;
  
  printf("value of SI %d",SI);
  
  return 0;
}


//Q5 Write a Program to calculate the Average of five Subject marks.

#include<stdio.h>

int main()
{
  int num1, num2, num3, num4, num5;
  
  scanf("%d%d%d%d%d",&num1, &num2, &num3, &num4, &num5);
  
  int average = (num1 + num2 + num3 + num4 + num5) / 5;
  
  printf("average is %d",average);
  
  return 0;
}
