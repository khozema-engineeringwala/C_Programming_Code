#include <stdio.h>
int main()
{
    for(int i=0; i<8; i++){
        for(int j=0;j<8;j++){
            if(j+i>=7)   
                printf(" *# ",i,j);
            else
                printf("    ");
        }
        for(int j=0;j<i;j++){
                printf(" *# ",i,j);
        }
        printf("\n");
    }

    return 0;
}
