 #inclide<stdio.h>
 int main()
 int flag=1;
    for(int i=0; i<6; i++){
        for(int j=0;j<=i;j++)
            printf(" ");
            
        for(int j=((6*2)-(i*2)-1);j>0;j--){
            if(((i+j)%2)==flag)
                printf("%d%d",i,j);
            else
                printf(" ");
        }
        
        flag=!flag;
        printf("\n");
    }
